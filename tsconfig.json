{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es5",
    "rootDir": "src",
    "outDir": "dist",
    "downlevelIteration": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "noImplicitReturns": true,
    "strict": true,
    "pretty": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "three-orbit-controls": [
        "node_modules/@types/three"
      ]
    },
    "declaration": true
  }
}
